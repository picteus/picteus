{
  "id": "automatic1111",
  "version": "0.1.0",
  "name": "Automatic1111",
  "description": "Interacts with Automatic1111.",
  "runtimes": [
    {
      "environment": "node"
    }
  ],
  "instructions": [
    {
      "events": ["process.started", "image.created", "image.updated", "image.computeTags", "image.computeFeatures"],
      "throttlingPolicies": [
        {
          "events": ["image.created", "image.updated", "image.computeTags", "image.computeFeatures"],
          "maximumCount": 10
        }
      ],
      "capabilities": [
        {
          "id": "image.tags"
        },
        {
          "id": "image.features"
        }
      ],
      "execution": {
        "executable": "${node}",
        "arguments": ["./dist/main.js"]
      }
    }
  ],
  "settings": {
    "type": "object"
  }
}

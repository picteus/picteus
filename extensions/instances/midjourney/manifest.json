{
  "id": "midjourney",
  "version": "1.0.0",
  "name": "Midjourney",
  "description": "Accesses to all sorts of Midjourney features.",
  "runtimes": [
    {
      "environment": "node"
    }
  ],
  "instructions": [
    {
      "events": [
        "process.started", "process.runCommand", "image.created", "image.updated", "image.computeTags",
        "image.computeFeatures"
      ],
      "throttlingPolicies": [
        {
          "events": ["image.created", "image.updated", "image.computeTags", "image.computeFeatures"],
          "maximumCount": 8
        }
      ],
      "capabilities": [
        {
          "id": "image.tags"
        },
        {
          "id": "image.features"
        }
      ],
      "execution": {
        "executable": "${node}",
        "arguments": [
          "./dist/main.js"
        ]
      },
      "commands": []
    }
  ],
  "ui": {
    "elements": [
      {
        "anchor": "window",
        "url": "https://www.midjourney.com"
      }
    ]
  },
  "settings": {
    "type": "object",
    "properties": {},
    "required": []
  }
}

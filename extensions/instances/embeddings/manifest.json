{
  "id": "embeddings",
  "version": "0.1.0",
  "name": "Embeddings",
  "description": "Computes the embeddings of an image.",
  "runtimes": [
    {
      "environment": "python"
    }
  ],
  "instructions": [
    {
      "events": [
        "process.started", "image.created", "image.updated", "image.computeEmbeddings", "text.computeEmbeddings"
      ],
      "throttlingPolicies": [
        {
          "events": ["image.created", "image.updated", "image.computeEmbeddings", "text.computeEmbeddings"],
          "durationInMilliseconds": 250,
          "maximumCount": 4
        }
      ],
      "capabilities": [
        {
          "id": "image.embeddings"
        },
        {
          "id": "text.embeddings"
        }
      ],
      "execution": {
        "executable": "${venvPython}",
        "arguments": ["./main.py"]
      }
    }
  ],
  "settings": {
    "type": "object"
  }
}

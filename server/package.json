{
  "name": "@picteus/server",
  "version": "0.7.0",
  "productName": "Picteus server",
  "description": "The Picteus back-end server.",
  "keywords": [
    "picture",
    "image",
    "lens",
    "voyager",
    "explorer",
    "search",
    "Odysseus"
  ],
  "author": {
    "name": "Ã‰douard Mercier",
    "email": "edouard@koppasoft.com",
    "url": "https://github.com/edouardmercier"
  },
  "license": "GNU Affero General Public License",
  "private": false,
  "engines": {
    "node": ">=22.17.1"
  },
  "homepage": "https://github.com/picteus/picteus",
  "repository": {
    "type": "git",
    "url": "https://github.com/picteus/picteus.git"
  },
  "bugs": {
    "url": "https://github.com/picteus/Picteus/issues"
  },
  "config": {
    "buildDirectoryPath": "../build",
    "storageDirectoryPath": "../storage",
    "databaseFileName": "database.db",
    "referenceDatabaseUrl": "file:../database.db"
  },
  "type": "module",
  "scripts": {
    ".env": "cross-var node -e \"fs.writeFileSync('.env', 'LOCAL_DATABASE_URL=file:' + path.resolve(process.cwd(), '$npm_package_config_storageDirectoryPath', '$npm_package_config_databaseFileName') + '\\n' + 'NO_DE_MODULES_DIRECTORY_PATH=' + path.resolve(process.cwd(), 'node_modules') + '\\n', { encoding: 'utf8'});\"",
    "reset": "npm run clean && shx rm -rf node_modules",
    "clean": "shx rm -rf \"src/generated\"",
    "updateVersion": "gulp updateVersion",
    "compile": "tsc && tsc-alias --resolve-full-paths && cross-var shx mkdir -p \"$npm_package_config_buildDirectoryPath/server/src/generated/prisma-client\" && cross-var shx cp \"src/generated/prisma-client/*.node\" \"$npm_package_config_buildDirectoryPath/server/src/generated/prisma-client\" && cross-var node -e \"const object = {}; object.type = 'module'; fs.writeFileSync(path.join('$npm_package_config_buildDirectoryPath', 'server', 'package.json'), JSON.stringify(object), { encoding: 'utf8'});\"",
    "obfuscate": "cross-var javascript-obfuscator \"$npm_package_config_buildDirectoryPath/server/src\" --output \"$npm_package_config_buildDirectoryPath/server/src\" --compact true --options-preset high-obfuscation --rename-globals true --self-defending true -- target node && shx rm -rf \"$npm_package_config_buildDirectoryPath/server/src/**/*.map\"",
    "generateMetadata": "ts-node generate-metadata.ts src",
    "installNodeModulesForElectron": "gulp installNodeModulesForElectron",
    "installNodeModulesForDockerfile": "gulp installNodeModulesForDockerfile",
    "prisma:studio:dotenv": "cross-var cross-env DATABASE_URL=\"$LOCAL_DATABASE_URL\" prisma studio --browser none",
    "preprisma:studio": "npm run .env",
    "prisma:studio": "dotenv -- npm run prisma:studio:dotenv",
    "prisma:generate": "prisma generate --",
    "prisma:fix": "gulp fixPrisma",
    "prisma:migrate:dotenv": "cross-var cross-env DATABASE_URL=\"$LOCAL_DATABASE_URL\" prisma migrate dev --skip-generate",
    "preprisma:migrate": "npm run .env",
    "prisma:migrate": "dotenv -- npm run prisma:migrate:dotenv",
    "prisma:update": "cross-var cross-env DATABASE_URL=\"$npm_package_config_referenceDatabaseUrl\" prisma migrate dev --skip-generate --name ' '",
    "prisma:seed": "cross-var cross-env DATABASE_URL=\"$npm_package_config_referenceDatabaseUrl\" prisma db seed",
    "prisma:copyMigrations": "gulp copyPrismaMigrations",
    "build:copy": "gulp copySecrets && gulp copyDatabase && gulp copyAssets && npm run prisma:copyMigrations",
    "build:generate": "npm-run-all prisma:generate openApi:generateAndPackage",
    "build:darwin:x64": "cross-env OPERATING_SYSTEM=darwin ARCHICTURE=x64 npm run build",
    "build:darwin:arm64": "cross-env OPERATING_SYSTEM=darwin ARCHICTURE=arm64 npm run build",
    "build:windows:x64": "cross-env OPERATING_SYSTEM=win32 ARCHICTURE=x64 npm run build",
    "build:windows:arm64": "cross-env OPERATING_SYSTEM=win32 ARCHICTURE=arm64 npm run build",
    "build": "npm-run-all installNodeModulesForElectron build:copy prisma:generate compile obfuscate",
    "start": "cross-var cross-env NODE_PATH=\"$NO_DE_MODULES_DIRECTORY_PATH\" REFERENCE_DATABASE_FILE_PATH=\"./database.db\" SDK_DIRECTORY_PATH=\"$npm_package_config_buildDirectoryPath/sdk\" node --max-http-header-size=131072 $npm_package_config_buildDirectoryPath/server/src/main.js --storageDirectoryPath \"$npm_package_config_storageDirectoryPath\" --webDirectoryPath \"$npm_package_config_buildDirectoryPath/web\"",
    "start:help": "cross-var cross-env NODE_PATH=\"$NO_DE_MODULES_DIRECTORY_PATH\" REFERENCE_DATABASE_FILE_PATH=\"./database.db\" SDK_DIRECTORY_PATH=\"$npm_package_config_buildDirectoryPath/sdk\" node $npm_package_config_buildDirectoryPath/server/src/main.js run --help",
    "start:custom": "cross-var cross-env NODE_PATH=\"$NO_DE_MODULES_DIRECTORY_PATH\" REFERENCE_DATABASE_FILE_PATH=\"./database.db\" SDK_DIRECTORY_PATH=\"$npm_package_config_buildDirectoryPath/sdk\" node --max-http-header-size=131072 $npm_package_config_buildDirectoryPath/server/src/main.js --storageDirectoryPath \"$npm_package_config_storageDirectoryPath\" --unpackedExtensionsDirectoryPath ../extensions/instances --webDirectoryPath \"$npm_package_config_buildDirectoryPath/web\" --useSsl false --requiresApiKeys false",
    "compileAndStart": "npm-run-all compile start",
    "ci:test:dotenv": "cd tests && cross-var cross-env NODE_OPTIONS=\"--experimental-vm-modules\" NODE_PATH=\"$NO_DE_MODULES_DIRECTORY_PATH\" jest --verbose --ci --runInBand --forceExit --colors --testPathIgnorePatterns \"inexistentSuite\" --testNamePattern \"^.*$\"",
    "preci:test": "npm run .env",
    "ci:test": "dotenv -- npm run ci:test:dotenv",
    "test": "cd tests && cross-env NODE_OPTIONS=\"--experimental-vm-modules\" jest --runInBand --verbose --colors --testNamePattern \"^.*$\"",
    "compileAndTest": "npm-run-all compile test",
    "openApi:generateOpenApi": "npm run compile && node \"$npm_package_config_buildDirectoryPath/server/src/main.js\" generateOpenApi --filePath openapi.json",
    "openApi:typeScript:generate": "gulp generateTypeScriptOpenApiClient",
    "openApi:javaScript:generate": "gulp generateJavaScriptOpenApiClient",
    "openApi:python:generate": "gulp generatePythonOpenApiClient",
    "openApi:java:generate": "gulp generateJavaOpenApiClient",
    "openApi:typeScript:package": "gulp packageTypeScriptOpenApiClient",
    "openApi:javaScript:package": "gulp packageJavaScriptOpenApiClient",
    "openApi:python:package": "gulp packagePythonOpenApiClient",
    "openApi:typeScript:generateAndPackage": "npm-run-all openApi:typeScript:generate openApi:typeScript:package",
    "openApi:javaScript:generateAndPackage": "npm-run-all openApi:javaScript:generate openApi:javaScript:package",
    "openApi:python:generateAndPackage": "npm-run-all openApi:python:generate openApi:python:package",
    "openApi:generateAndPackage": "npm-run-all openApi:typeScript:generateAndPackage openApi:python:generateAndPackage",
    "openApi:typeScript:install": "gulp installTypeScriptOpenApiClient"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "shx": "0.4.0",
    "cross-env": "10.0.0",
    "cross-var": "1.1.0",
    "dotenv-cli": "10.0.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/testing": "11.1.9",
    "javascript-obfuscator": "4.1.1",
    "@openapitools/openapi-generator-cli": "2.25.0",
    "del": "5.1.0",
    "gulp": "4.0.2",
    "gulp-run": "1.7.1",
    "gulp-replace": "1.1.4",
    "through2": "4.0.2",
    "prettier": "3.6.2",
    "prisma": "6.19.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "tsc-alias": "1.8.16",
    "@types/node": "22.17.1",
    "@types/compression": "1.8.1",
    "@types/http-codes": "1.0.4",
    "@types/plist": "3.0.5",
    "@types/heic-decode": "1.1.3",
    "@types/adm-zip": "0.5.7",
    "@types/semver": "7.7.1",
    "@types/tar-fs": "2.0.4",
    "@types/chance": "1.1.7",
    "jest": "30.2.0",
    "wait-for-expect": "4.0.0",
    "ts-jest": "29.4.5",
    "@types/jest": "30.0.0",
    "fast-cartesian": "9.0.1",
    "pick-port": "2.2.0",
    "socket.io-client": "4.8.1",
    "moment": "2.30.1",
    "crc-32": "1.2.2",
    "micromatch": "4.0.8"
  },
  "optionalDependencies": {
    "@picteus/ws-client": "file:../generated/openapi/typescript-fetch",
    "@picteus/internal-extension-sdk": "file:../extensions/sdk/typescript"
  },
  "dependencies": {
    "@babel/runtime": "7.28.4",
    "@nestjs/common": "11.1.9",
    "@nestjs/core": "11.1.9",
    "@nestjs/platform-express": "11.1.9",
    "@nestjs/swagger": "11.2.1",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/websockets": "11.1.9",
    "@nestjs/platform-socket.io": "11.1.9",
    "@nestjs/throttler": "6.4.0",
    "@prisma/client": "6.19.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "@samchon/openapi": "5.0.1",
    "nest-winston": "1.10.2",
    "rate-limiter-flexible": "8.3.0",
    "bottleneck": "2.19.5",
    "zod": "4.1.12",
    "compression": "1.8.1",
    "fdir": "6.4.6",
    "http-codes": "1.0.0",
    "http-constants": "2.1.0-19-gdd89f31",
    "mime-types": "3.0.1",
    "plist": "3.1.0",
    "chokidar": "4.0.3",
    "sharp": "0.34.5",
    "piexifjs": "1.0.6",
    "meta-png": "1.0.6",
    "exifremove": "1.0.1",
    "heic-decode": "2.1.0",
    "exif-js": "2.3.0",
    "exif-reader": "2.0.2",
    "exifr": "7.1.3",
    "icc": "3.0.0",
    "exif-be-gone": "1.5.1",
    "fast-xml-parser": "5.3.5",
    "piscina": "5.1.4",
    "cross-spawn": "7.0.6",
    "ajv": "8.17.1",
    "adm-zip": "0.5.16",
    "chromadb": "3.1.4",
    "@chroma-core/default-embed": "0.1.9",
    "file-type-mime": "0.4.7",
    "tar-fs": "3.1.1",
    "semver": "7.7.3",
    "chance": "1.1.13",
    "@picteus/shared-back-end": "file:../shared/back-end"
  },
  "overrides": {
    "libphonenumber-js-saved": "npm:dry-uninstall"
  }
}
